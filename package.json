{
  "name": "runcc",
  "version": "0.1.6",
  "description": "CC launcher with endpoint switching support",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "runcc": "dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "build:bin": "bun build --compile src/index.ts --outfile runcc",
    "dev": "bun run src/index.ts",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "test:unit": "bun test src/__tests__/unit/**/*.test.ts",
    "test:integration": "bun test src/__tests__/integration/**/*.test.ts",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "bun-types": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ooneko/cc-run.git"
  },
  "keywords": [
    "claude",
    "code"
  ],
  "author": "binhong.hua",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ooneko/cc-run/issues"
  },
  "homepage": "https://github.com/ooneko/cc-run#readme"
}
